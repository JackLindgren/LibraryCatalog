<h1>Authors</h1>
<table>
	<tr>
		<th>Title</th>
		<th>First Name</th>
		<th>Last Name</th>
		<th>Language</th>
		<th>Country</th>
		<th>Year</th>
	</tr>

	{{#each books}}
	<tr>
		<td>{{this.title}}</td>
		<td>{{this.firstName}}</td>
		<td>{{this.lastName}}</td>
		<td>{{this.language}}</td>
		<td>{{this.country}}</td>
		<td>{{this.year}}</td>
	<tr>
	{{/each}}
</table>