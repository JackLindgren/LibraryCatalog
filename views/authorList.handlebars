<h1>Authors</h1>
<a href="/listBooks">Books</a>
<a href="/listAuthors">Authors</a>
<a href="/addBook">Add Book</a>
<a href="/addAuthor">Add Author</a>
<table>
	<tr>
		<th>Last Name</th>
		<th>First Name</th>
		<th>Birthdate</th>
		<th>Country</th>
	</tr>

	{{#each authors}}
	<tr>
		<td>{{this.lastName}}</td>
		<td>{{this.firstName}}</td>
		<td>{{this.dob}}</td>
		<td>{{this.country}}</td>
	<tr>
	{{/each}}
</table>